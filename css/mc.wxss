.absoulte {
    position: absolute;
}

.absolute {
    position: absolute;
}

.absoulte.full,
.fixed.full,
.absolute.full {
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.relative {
    position: relative;
}

.fixed {
    position: fixed;
}

.userImage {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    background-color: #F1F1F1;
    overflow: hidden;
}


